.wrap{
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
}
.item{
  padding: 35rpx;
  background-color: #ffffff;
  margin: 20rpx; 
  border-radius:20rpx; 
}
.item_a{
  display: flex;
  justify-content: space-between;
  flex-direction:row;
  background-color: #ffffff;
  align-items: center;
  
}
.title{
  width: 100%;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  justify-content:center;
  align-items: center;
}
.footer{
  position: fixed;
  bottom: 0px;
  width: 100%;
  height: 180rpx;
  background-color: #ffffff;
  display: flex;
  justify-content: space-around;
  flex-direction:row;
  align-items: center;
  border-top: 2rpx solid gray;
}
.footer icon{
  display: block;
  margin-bottom: 15rpx;
}
.footer view{
  text-align: center
}
.item_b view{
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
  border-bottom:2rpx solid gray;
  font-size: 50rpx;
  color: gray; 
}