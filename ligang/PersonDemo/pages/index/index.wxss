.container{
    width:100%;
    height:300px;
    text-align: center;
    border: 1px solid red;
    margin-top:20px; 
    background-color: #aed8e6;
    box-shadow: 10px 10px 5px #888888;
}
.ripple {
    background-color: rgba(0, 0, 0, 0.8);
    border-radius: 100%;
    color: yellow;
    height:10px;
    width:10px;
    margin-top: -90px;
    position: absolute;
    -webkit-transform: scale(0);
}
@-webkit-keyframes ripple {
    100% {
    -webkit-transform: scale(5);
    transform: scale(5);
    background-color: transparent;
    }
}
.center{
  text-align: center;
  font-size:50rpx; 
}

.img-plus-style {  
  height: 150rpx;  
  width: 150rpx;  
  position: absolute;  
  bottom: 250rpx;  
  right: 100rpx;  
  z-index: 100;  
}  
  
.img-style {  
  display: inline-block;
  height: 150rpx;  
  width: 150rpx;  
  position: absolute;  
  bottom: 250rpx;  
  right: 100rpx;  
  opacity: 0;  
}  
.clickWrap{
  display: block;
  height: 250px;
  width: 100%;
  background-color: #aed8e6;
}
.psize{
  width: 170rpx;
  height: 170rpx;
}