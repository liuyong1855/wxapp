/**index.wxss**/
.head{
  width: 750rpx;
  height: 480rpx;
  position: relative
}
.headbgwrap{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 1;
}
.headContent{
  width: 710rpx;
  height: 400rpx;
  position: absolute;
  z-index: 2;
  left: 20rpx;
  top: 20rpx;
}
.logo{
  display: block;
  width: 240rpx;
  height: 80rpx;
  margin: 20rpx auto;
}
.headinputwrap{
  position: relativeï¼›
}
.headinput{
  background-color: #ffffff;
  width: 650rpx;
  height: 80rpx;
  border-radius:10rpx; 
  padding-left: 60rpx;
}
.headinputwrap image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left: 5rpx;
  top: 135rpx;
}
.headList{
  display: flex;
  justify-content: space-around;
  margin-top:50rpx 
}
.headList view{
  width: 130rpx;
  text-align: center;
}
.headList view  text{
  display: block;
  font-size: 20rpx;
  color: #ffffff;
}
.headList image{
  width: 100rpx;
  height: 100rpx;
}
.chufang{
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content:space-between;
  background-color: #f5f5f5;
  align-items:center;
}
.chufang  image{
  width: 40rpx;
  height: 35rpx;
}
.chufang text{
  font-size:30rpx;
  color: blue;
}
.chufang view:nth-of-type(1){
  margin-left: 20rpx;
}
.chufang view:nth-of-type(2){
  margin-right: 20rpx;
}
.pinpai{
  width: 100%;
  display: flex;
  height: 80rpx;
  align-items: center;
  justify-content:space-between;
  background-color: #ffffff;
  border-top:5rpx solid #d4d4d4; 
  border-bottom:5rpx solid #d4d4d4; 
}
.pinpai text{
  margin-left: 20rpx;
}
.pinpai :nth-child(2){
  margin-right: 20rpx;
}
.pinpailist{
  padding: 20rpx;
}
.pinpailist image{
  width: 230rpx;
  height: 100rpx
}
.shan{
  padding: 20rpx;
  margin-top:20rpx;
  height: 70rpx;
}
.shan image{
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: top;
  margin: 0px 15rpx;
}
.time{
  display: inline-block;
  vertical-align: top;
}
.time text:nth-child(odd){
  padding: 10rpx;
  background-color: #c9c9c9c9;
  border-radius:15rpx;
}
.time text:nth-child(even){
  padding: 10rpx;
}
.jian{
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  border-right:10rpx solid #c9c9c9;
  border-bottom:10rpx solid #c9c9c9;  
  transform: rotateZ(-45deg); 
  float: right;
  margin-top: 10rpx;
}
.middle{
  height: 30rpx;
  background-color: #f5f5f5;
  border-top:3rpx solid #d4d4d4; 
  border-bottom:3rpx solid #d4d4d4; 
}
.jijiang{
  height: 250rpx;
  display: flex
}
.jijiang image{
  flex: 1;
  border: 3px solid #d4d4d4;
  height: 250rpx;
}
.changyong{
  padding: 20rpx;
  color: #444444;
  font-size: 40rpx;
  border-top:1rpx solid #d4d4d4; 
  border-bottom:1rpx solid #d4d4d4; 
}
.jiBingList{
  padding: 20rpx;
}
.jiBingBox{
  width: 100%;
  height: 220rpx;
}
.left{
  width: 220rpx;
  height: 220rpx;
  display: inline-block;
}
.left image{
  width: 220rpx;
  height: 220rpx;

}
.right{
  display: inline-block;
  vertical-align: top;
  width:470rpx;
  height:220rpx;
}
.right view{
  display: inline-block;
  width: 210rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  margin-bottom: 20rpx;
  margin-left: 20rpx;
  background-color: #f5f5db;
  color: #f58c3b;
  font-size: 30rpx;

}
.footer{
  position: fixed;
  bottom: 0px;
  background-color: #f6f8f8;
  width: 100%;
  display: flex;
  justify-content:space-around;
  border-top: 2px solid #d4d4d4;
  border-top: 2px solid #d4d4d4;
}
.footer view{
  width: 100rpx;
  height: 120rpx;
  text-align: center;
  margin-top: 20rpx;

}
.footer image{
  display: block;
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto;
}
.footer text{
  font-size: 30rpx;
  color: #a9aaaa;
}

