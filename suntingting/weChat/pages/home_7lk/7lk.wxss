/**7lk.wxss**/

page {
  background: #f5f5f5;
}

.container {
  padding: 0;
  font-size: 14px;
  background: #f5f5f5;
}

.clearfix:after {
  content: "";
  height: 0;
  line-height: 0;
  display: block;
  visibility: hidden;
  clear: both;
}

.row1 {
  width: 100%;
  height: 250px;
  background: url('../images/logoBg.jpg')no-repeat center center;
  background-size: cover;
}

.row1Log {
  width: 35%;
  height: 27%;
  margin: 20px auto;
  display: block;
}

.row1Search {
  margin-bottom: 20px;
  padding: 8px 0 8px 30px;
  background: #fff url(../images/logoSearch.png)no-repeat 5px center;
  color: #333;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
  background-size: auto 55%;
  box-sizing: border-box;
  margin: 0 10px;
}

.row1Icon {
  display: flex;
  padding: 20px 10px 0;
  box-sizing: border-box;
}

.rowLi {
  flex: 1;
  width: 25%;
}

.rowLi image {
  display: block;
  width: 45px;
  height: 45px;
  margin: 0 auto;
}

.rowLi text {
  color: #fff;
  font-size: 16px;
  text-align: center;
  width: 100%;
  display: block;
  padding-top: 3px;
}

.row1bot {
  width: 100%;
  font-size: 16px;
  height: 42px;
  line-height: 42px;
  color: #2394da;
  padding: 0 10px;
  box-sizing: border-box;
  background: #f5f5f5;
}

.row1my {
  float: left;
  padding-left: 30px;
  background: url(../images/logoIcon.png)no-repeat left center;
  background-size: 24px;
}

.row1add {
  float: right;
  padding-left: 30px;
}

.row2Tit {
  width: 100%;
  height: 42px;
  line-height: 42px;
  padding: 0 10px;
  box-sizing: border-box;
  background: #fff;
  border-bottom: 1px solid #dcdcdc;
}

.row2t, .row3t {
  font-size: 20px;
  color: #444;
  float: left;
}

.row2arr, .row3arr {
  width: 15px;
  /*height: 42px;*/
  display: block;
  float: right;
  padding-top: 17px;
  box-sizing: border-box;
  background-color: white;
}

.row2arr:before, .row3arr:before {
  content: " ";
  width: 15px;
  height: 5px;
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  transform: rotate(45deg);
  background: #c9c9c9;
  display: block;
}

.row2arr:after, .row3arr:after {
  content: " ";
  width: 15px;
  height: 5px;
  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  transform: rotate(-45deg);
  background: #c9c9c9;
  display: block;
}

.row2 {
  padding: 8px;
  margin-bottom: 10px;
  background: #fff;
  clear: both;
  overflow: hidden;
}

.row2Ul view {
  width: 32%;
  float: left;
  margin-bottom: 2%;
}

.row2Ul image {
  width: 100%;
  height: 90px;
}

.row3 {
  margin-bottom: 10px;
  background: #fff;
}

.row3Tit, .row4Tit {
  height: 42px;
  line-height: 42px;
  padding: 0 10px;
  box-sizing: border-box;
  border-bottom: 1px solid #dcdcdc;
}

.row3Time {
  float: left;
  padding-left: 15px;
}

.row3Time text {
  color: #fff;
  background: #999;
  padding: 2px 5px;
  margin: 0 5px 0 2px;
}

.row3Con .row3Li {
  width: 33%;
  float: left;
  border-right: 1px solid #dcdcdc;
}

.row3Con {
  overflow: hidden;
}

.row3Con .row3Li image {
  width: 100%;
  padding: 5px 8px;
  box-sizing: border-box;
  height: 140px;
}

.row3Con .row3Li .row3_p {
  width: 100%;
  padding: 2px 8px;
  box-sizing: border-box;
  overflow: hidden;
}

.row3Con .row3Li .row3_p text {
  max-width: 50%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
}

.row3Con .row3Li .row3_p text:nth-child(1) {
  text-align: left;
  color: #e60044;
  font-size: 14px;
  line-height: 30px;
}

.row3Con .row3Li .row3_p text:nth-child(2) {
  float: right;
  padding: 0 5px;
  background: #e60044;
  color: #fff;
  margin-top: 5px;
}

.row3Con .row3Li:last-child {
  border: none;
}

.row4 {
  background: #fff;
  margin-bottom: 10px;
}

.row4Tit {
  font-size: 20px;
  color: #444;
}

.row4Con {
  padding: 8px;
}

.row4Li {
  display: flex;
  margin-bottom: 10px;
}

.row4L {
  position: relative;
  width: 96px;
  height: 102px;
  text-align: center;
  line-height: 100px;
  font-size: 18px;
  overflow: hidden; /*flex: 0.75; *//*margin-right: 7px;*/
}

.row4R {
  width: 100%;
  margin: 0;
  flex: 2;
  margin-top: -4px;
  padding-left: 4px;
  box-sizing: border-box;
}

.row4L image {
  width: 100px;
  height: 102px;
}

.row4R .row4sLi {
  float: left;
  display: block;
  text-align: center;
  height: 47px;
  line-height: 47px;
  color: #f58c3b;
  width: 47%;
  background: #f5f5db;
  margin: 1.5% 0 1.5% 1.5%;
}

.row4L text {
  position: absolute;
  color: #e87816;
  display: block;
  width: 100%;
  text-align: center;
  font-size: 18px;
  top: 0;
  left: 0;
}

.row5 {
  padding: 14px 10px 14px;
}

.row5Phone {
  background: #481c87;
  text-align: center;
  padding: 7px 5px;
  margin-bottom: 10px;
  border-radius: 3px;
}

.row5Phone wx-text {
  font-size: 18px;
  color: #fff;
  background: url(../images/rowphone.png)no-repeat;
  background-size: 20px;
  padding-left: 26px;
}

.row5T {
  font-size: 12px;
  width: 100%;
  text-align: center;
  color: #999;
  line-height: 20px;
}

.row5T navigator {
  text-decoration: none;
  margin-left: 5px;
  display: inline-block;
}

.row6box {
  width: 100%;
  height: 50px;
}

.row6 {
  width: 100%;
  height: 50px;
  background-color: #eee;
  position: fixed;
  bottom: 0;
  left: 0;
  color: #707070;
  z-index: 999;
  border-top: solid 1px #ddd;
  padding-top: 5px;
  box-sizing: border-box;
}

.row6Li {
  width: 25%;
  float: left;
  text-align: center;
  height: 50px;
}

.row6Img,.row6Img2,.row6Img3,.row6Img4 {
  width: 22px;
  height: 20px;
  display: block;
  margin-left: auto;
  margin-right: auto;
  background: url(../images/indexBotton.png) no-repeat;
  background-size: 640% auto;
}

.row6Img {
  background-position: 0 top;
}

.row6Img2 {
  background-position: -22px top;
}

.row6Img4{
  background-position: -47px top;
}

.row6Img4 {
  background-position: -68px top;
}

.row6Li text {
  font-size: 13px;
}
